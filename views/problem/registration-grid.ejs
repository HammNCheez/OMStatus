<% for(var i=0 ; i < venues.length; i++) { %>
    <h3>Venue <%= venues[i][0].venue %></h3>
    <table class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>Membership #</th>
                <th>Team</th>
                <th><a href="/problem/<%= probNum %>?sort=longTerm">LT</a>/<a href="/problem/<%= probNum %>?sort=spontaneous">Spon</a>
                </th>
                <% if(Utils.hasRole(session.user, 'registration')) { %>
                    <th></th>
                    <% } %>
            </tr>
        </thead>
        <tbody>
            <% for(var j=0 ; j < venues[i].length; j++) { %>
                <tr <%- (venues[i][j].scoresReady) ? 'class="warning"': '' %>>
                    <td>
                        <%= venues[i][j].membershipNumber %>
                    </td>
                    <td>
                        <a href="/team/show/<%= venues[i][j].id %>">
                            <%= venues[i][j].schoolName %>
                                <% if(venues[i][j].teamLetter) { %>
                                    :
                                    <%= venues[i][j].teamLetter %>
                                        <% } %>
                        </a>
                        <br>
                        <%= venues[i][j].division %>
                    </td>
                    <td>
                        <%if (!venues[i][j].performed) { %>
                            <%= Utils.getTime(venues[i][j].longtermTime) %>
                        <% } else { %>
                            <span class="glyphicon glyphicon-ok"></span>
                        <% } %>
                         /
                        <%if (!venues[i][j].sponCompleted) { %>
                            <%= Utils.getTime(venues[i][j].sponTime) %>
                        <% } else { %>
                            <span class="glyphicon glyphicon-ok"></span>
                        <% } %>
                    </td>
                    <% if(Utils.hasRole(session.user, 'registration')) { %>
                        <td>
                            <% if(!venues[i][j].checkedIn) { %>
                                <a class="btn btn-success btn-xs" href="/team/checkIn/<%= venues[i][j].id %>?problem=<%= probNum %>">Check-in</a>
                                <% } %>
                        </td>
                        <% } %>
                </tr>
                <% } %>
        </tbody>
    </table>
    <% } %>
