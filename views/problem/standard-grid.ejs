<% for(var i=0 ; i < venues.length; i++) { %>
    <h3>Venue <%= venues[i][0].venue %></h3>
    <table class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>Team</th>
                <th><a href="/problem/<%= probNum %>?sort=longTerm">Longterm</a>
                </th>
                <th><a href="/problem/<%= probNum %>?sort=spontaneous">Spontaneous</a>
                </th>
            </tr>
        </thead>
        <tbody>
            <% for(var j=0 ; j < venues[i].length; j++) { %>
                <tr class="<%- (venues[i][j].scoresReady) ? ' warning ': '' %><%- (venues[i][j].noShow) ? ' danger ' : ''%>">
                    <td>
                        <a href="/team/show/<%= venues[i][j].id %>">
                            <%= venues[i][j].schoolName %>
                                <% if(venues[i][j].teamLetter) { %>
                                    :
                                    <%= venues[i][j].teamLetter %>
                                        <% } %>
                        </a>
                        <br>
                    </td>
                    <td class="autoFit">
                        <%if (!venues[i][j][k].performed) { %>
                            <%= Utils.getTime(venues[i][j][k].longtermTime) %>
                        <% } else { %>
                            <span class="glyphicon glyphicon-ok"></span>
                        <% } %>
                    </td>
                    <td class="autoFit">
                        <%if (!venues[i][j][k].sponCompleted) { %>
                            <%= Utils.getTime(venues[i][j][k].sponTime) %>
                        <% } else { %>
                            <span class="glyphicon glyphicon-ok"></span>
                        <% } %>
                    </td>
                    <% if(Utils.hasRole(session.user, 'registration')) { %>
                        <td class="autoFit">
                            <% if(!venues[i][j][k].checkedIn) { %>
                                <a class="btn btn-success btn-xs" href="/team/checkIn/<%= venues[i][j][k].id %>?problem=<%= probNum %>">Check-in</a>
                                <% } %>
                        </td>
                        <% } %>
                </tr>
                <% } %>
        </tbody>
    </table>
    <% } %>
