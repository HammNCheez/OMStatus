<h1>Problem <%= probNum %></h1>

<% for(var i = 0; i < venues.length; i++) { %>
<h3>Venue <%= venues[i][0].venue %></h3>
<table border=1>
	<thead>
		<tr>
			<th>Membership #</th>
			<th>Team</th>
			<th>Longterm</th>
			<th>Spontaneous</th>
		</tr>
	</thead>
	<tbody>
		<% for(var j = 0; j < venues[i].length; j++) { %>
			<tr>
				<td><%= venues[i][j].membershipNumber %></td>
				<td><a href="/team/show/<%= venues[i][j].id %>"><%= venues[i][j].schoolName %> 
				<% if(venues[i][j].teamLetter) { %>
					: <%= venues[i][j].teamLetter %>
				<% } %>
				</a><br>
					<%= venues[i][j].division %></td>
				<td><%= moment(venues[i][j].longtermTime).format('h:mm a') %></td>
				<td><%= moment(venues[i][j].sponTime).format('h:mm a')  %></td>
			</tr>
		<% } %>
	</tbody>
</table>
<% } %>

<a href="/team/new?problem=<%= probNum %>">Add New Team</a>